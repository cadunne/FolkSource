<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.citizensense.model">
<class name="Submission" table="task_submissions">
	<id name="id" column="id">
		<generator class="native" />
	</id>
<!-- 	<many-to-one name="task" column="task_id" /> -->
<!-- 	<property name="task_id" /> -->
<!-- 	<property name="notes" /> -->
	<property name="gps_location" />
	<list name="answers" lazy="false">
		<key column="id" not-null="false"/>
		<index column="sub_id" />
		<one-to-many class="Answer" />
	</list>
</class>
</hibernate-mapping>