<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.citizensense.model">
<class name="Attributes" table="attributes" >
	<id name="id" column="id">
		<generator class="native" />
	</id>
	<property name="attr_property"/>
	<many-to-one name="question_id" class="Question" 
		column="id" not-null="true" />
	<many-to-one name="type_id" class="Types"
		column="id"	not-null="true" />
		
</class>
</hibernate-mapping>