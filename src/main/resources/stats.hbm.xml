<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.citizensense.model" >
	<class name="Stats" table="stats" >
		<id name="id" column="id" >
			<generator class="native" />
		</id>
		<property name="task_id" />
		<property name="loc_id" />
		<property name="num_submissions" />
		<property name="num_sbs_processed" />
		<property name="curr_confidence" />
	</class>
</hibernate-mapping>